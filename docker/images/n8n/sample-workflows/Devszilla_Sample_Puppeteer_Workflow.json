{
  "name": "Devszilla Sample Puppeteer Workflow",
  "nodes": [
    {
      "parameters": {
        "jsCode": "const puppeteer = require('puppeteer-core');\n\ntry {\n  const browser = await puppeteer.launch({\n    headless: 'new',\n    executablePath: '/usr/bin/chromium-browser',\n    args: ['--no-sandbox', '--disable-setuid-sandbox'],\n  });\n\n  const page = await browser.newPage();\n  await page.goto('https://www.google.com');\n\n  const screenshot = await page.screenshot({ encoding: 'base64' }); // Capture screenshot as base64\n\n  await browser.close();\n\n  return [{ success: true, screenshot: screenshot }]; // Return the screenshot\n\n} catch (error) {\n  return [{ success: false, error: error.message }]; // Return error information\n}"
      },
